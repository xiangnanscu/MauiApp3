@page "/counter"

<h1>Counter</h1>

<p role="status">!!!yyxxxxxxxxxxxxxxx!xxxxxCurrent count: @currentCount</p>

<button class="btn btn-primary" @onclick="IncrementCount">Click mee</button>

@code {
    private int currentCount = 0;

    private void IncrementCount()
    {
        currentCount++;
    }
}
